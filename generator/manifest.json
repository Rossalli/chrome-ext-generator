{
  "name": "Gerador de Dados no Botão Direito",
  "description": "Gera dados no campo de texto utilizando o botao direito. Simples, rápido e fácil.",
  "version": "0.2",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["index.js", "utils.js"]
  },
  "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "manifest_version": 2
}
